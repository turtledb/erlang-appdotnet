{deps, [
        {ibrowse, "4.0.1", {git, "git://github.com/cmullaparthi/ibrowse.git", {tag, "v4.0.1"}}},
        {jsx, "1.3.1", {git, "git://github.com/talentdeficit/jsx.git", {tag, "v1.3.1"}}}
       ]}.
{erl_opts, [fail_on_warning,debug_info,{d,debug}]}.
{sub_dirs, ["rel"]}.
{lib_dirs, [".","deps"]}.
{cover_enabled, true}.
{clean_files, ["*.eunit", "ebin/*.beam"]}.
{eunit_compile_opts, [{src_dirs, ["src"]}]}.
{eunit_opts, [verbose, {report, {eunit_surefire, [{dir, "."}]}}]}.
